<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.springfever">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.SpringFever"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".GaleryActivity"
            android:label="@string/title_gallery"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeTwo" />
        <activity
            android:name=".ShoppingActivity"
            android:label="@string/title_shopping"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeOne" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_about"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeThree" />
        <activity
            android:name=".ViewSingleCallActivity"
            android:label="@string/title_view_single_call"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeOne" />
        <activity
            android:name=".ViewCallsActivity"
            android:label="@string/title_view_calls"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeOne" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.SpringFever.ThemeThree" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>